{
  "name": "mocha-earl-reporter",
  "engines": {
    "node": ">=4"
  },
  "version": "2.5.1",
  "description": "EARL reporter for mocha",
  "main": "./lib/teamcity.js",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "test": "mocha test/functional test/programmatic",
    "test-teamcity": "mocha test/functional test/programmatic --reporter lib/teamcity.js",
    "test-teamcity-example": "mocha test/example/exampleSimple.js --reporter lib/teamcity.js",
    "lint": "eslint lib/*.js test/**/*.js",
    "lint-fix": "eslint --fix lib/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travisjeffery/mocha-earl-reporter"
  },
  "keywords": [
    "mocha",
    "earl",
    "reporter",
    "jetbrains"
  ],
  "dependencies": {
    "mocha": ">=3.5.0"
  },
  "author": "Eric Prud'hommeaux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travisjeffery/mocha-earl-reporter/issues"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.4.1"
  }
}
